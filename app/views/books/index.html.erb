<h1>Books</h1>
<h4>Title</h4>
<h4>body</h4>
<% @books.each do |book| %>

　<%= book.title%>

　<%= book.body%>

　<%=link_to "Show",book_path(book.id) %>
  <%=link_to "Edit",edit_book_path(book.id) %>
　<%=link_to "Destroy",destroy_book_path(book.id),method: :delete, "data-confirm" => "本当に削除しますか？"%>
　<br>


<% end %>